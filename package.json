{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "0.36.4"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "build": "yarn install && yarn rw build && yarn rw prisma migrate deploy",
    "start": "node -r dotenv/config node_modules/@redwoodjs/api-server/dist/index.js -f api/dist/functions --port 8911"
  }
}
