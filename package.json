{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "^0.47.1"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.17 <=16.x",
    "yarn": "1.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "build": "yarn install && yarn rw build && yarn rw prisma migrate deploy",
    "update": "git pull && yarn build && yarn start",
    "serve": "yarn rw dev",
    "start": "yarn rw serve"
  },
  "version": "2.1.0"
}
