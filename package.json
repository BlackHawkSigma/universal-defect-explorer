{
  "private": true,
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/cli-storybook": "7.7.4",
    "@redwoodjs/core": "8.5.0"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": "=20.x",
    "yarn": "4.x"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "build": "yarn install && yarn rw build && yarn rw prisma migrate deploy",
    "update": "git pull && yarn build && yarn start",
    "serve": "yarn rw dev",
    "start": "yarn rw serve",
    "storybook": "yarn rw storybook"
  },
  "version": "2.2.0",
  "packageManager": "yarn@4.7.0"
}
